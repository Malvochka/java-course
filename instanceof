class Wrapper<T> {
    private T item;

    public Wrapper(T item) {
        this.item = item;
    }

    public T getItem() {
        return item;
    }
}

public class Main {
    public static void main(String[] args) {
        Wrapper<Integer> intWrapper = new Wrapper<>(10);
        Wrapper<String> stringWrapper = new Wrapper<>("Hello");

        // Пример использования instanceof
        if (intWrapper instanceof Wrapper<?>) {
            System.out.println("intWrapper является объектом параметризованного класса Wrapper");
        }

        if (stringWrapper instanceof Wrapper<?>) {
            System.out.println("stringWrapper является объектом параметризованного класса Wrapper");
        }
    }
}
